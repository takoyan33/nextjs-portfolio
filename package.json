    "next": "16.0.10",
    "pino": "^10.1.0",
    "prettier": "^3.3.3",
    "react": "19.0.1",
    "react-dom": "19.0.1",
    "react-hook-form": "^7.53.0",
    "react-modal": "^3.16.3",
    "react-scan": "^0.3.3",
    "sass": "^1.77.8",
