{
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3001",
    "dev:mock": "NEXT_PUBLIC_USE_MOCK=true next dev",
    "build": "rm -rf .next && next build --debug-prerender",
    "start": "next start",
    "lint": "eslint ./app --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint ./app --ext .ts,.tsx,.js,.jsx --fix",
    "lint:stylelint": "stylelint '**/*.{scss,sass}'",
    "lint:stylelint:fix": "stylelint --fix '**/*.{scss,sass}'",
    "format": "prettier --write ./app",
    "prepare": "husky",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "playwright": "playwright test",
    "playwright:ui": "playwright test --ui",
    "commitmsg": "commitlint --edit",
    "unlighthouse": "npx unlighthouse --site http://localhost:3001",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
